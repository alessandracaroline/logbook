<!DOCTYPE html>
<html>
  <head>
    <style type="text/css">
      html, body { height: 100%; margin: 0; padding: 0; }
      #map { height: 80%; }
    </style>
  </head>
  <body>
    <div id="map"></div>
    <script type="text/javascript">

var map;
function initMap() {
  var myLatLng = {lat: <%= log.latitude %>, lng: <%= log.longitude %>};

  var map = new google.maps.Map(document.getElementById('map'), {
    center: myLatLng,
    zoom: 10
  });

  var marker = new google.maps.Marker({
    position: myLatLng,
    map: map,
    title: 'Your log!'
  });
}

    </script>
    <script async defer
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBG2VZYITCFuKpzuASCNzjMVpzgmD29E50&callback=initMap">
    </script>
  </body>
</html>